<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="cardPane" minWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <HBox spacing="5.0">
      <Label fx:id="id" text="id"/>
      <Label fx:id="name" text="name"/>
   </HBox>
   <Label fx:id="description" text="description"/>
   <Label fx:id="deadline" text="deadline"/>
   <Label fx:id="completion" text="completion"/>
</VBox>
