<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <VBox alignment="CENTER_LEFT" minHeight="105" GridPane.columnIndex="0">
            <padding>
                <Insets top="5" right="5" bottom="5" left="5" />
            </padding>
            <Label fx:id="description" styleClass="cell_big_label" text="\$first" />
            <Label fx:id="project" styleClass="cell_small_label" text="\$project" />
            <Label fx:id="status" styleClass="cell_small_label" text="\$status" />
            <Label fx:id="priority" styleClass="cell_small_label" text="\$priority" />
            <Label fx:id="deadline" styleClass="cell_small_label" text="\$deadline" />
        </VBox>
    </GridPane>
</HBox>
