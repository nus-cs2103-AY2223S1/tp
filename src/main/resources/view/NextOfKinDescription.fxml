<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="nextOfKinDescription" alignment="CENTER_LEFT" minHeight="-Infinity" minWidth="-Infinity" prefHeight="95.0" prefWidth="270.0" spacing="1.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="name" prefHeight="17.0" prefWidth="135.0" styleClass="name-label" text="\$first" />
      <FlowPane fx:id="nokTags" prefHeight="0.0" prefWidth="250.0" />
      <HBox alignment="CENTER_LEFT" spacing="5">
         <children>
            <Label styleClass="prefix-label" text="Phone:" />
            <Label fx:id="phone" styleClass="cell_small_label" text="\$phone" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="5">
         <children>
            <Label styleClass="prefix-label" text="Address:" />
            <Label fx:id="address" styleClass="cell_small_label" text="\$address" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="5">
         <children>
            <Label styleClass="prefix-label" text="Email:" />
            <Label fx:id="email" styleClass="cell_small_label" text="\$email" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
   </padding>
</VBox>
