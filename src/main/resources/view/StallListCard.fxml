<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="96.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <GridPane prefHeight="150.0" prefWidth="350.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
        <VBox prefHeight="120.0" prefWidth="100.0">
          <children>
            <HBox prefHeight="30.0">
              <children>
                <Label fx:id="id" styleClass="cell_big_label" text="\$id" translateX="10.0" translateY="5.0" />
                <Label fx:id="name" prefHeight="18.0" prefWidth="285.0" snapToPixel="false" styleClass="cell_big_label" text="\$name" translateX="30.0" translateY="5.0" />
              </children>
            </HBox>
            <GridPane fx:id="gridPane">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.20001220703125" minWidth="10.0" prefWidth="73.6" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="276.79999999999995" minWidth="10.0" prefWidth="276.79999999999995" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
              <children>
                <Label fx:id="addressLabel" styleClass="cell_small_label" text="Address:" translateX="10.0" />
                <Label fx:id="tagsLabel" styleClass="cell_small_label" text="Tags:" translateX="10.0" GridPane.rowIndex="1" />
                <Label fx:id="address" styleClass="cell_small_label" translateX="10.0" wrapText="true" GridPane.columnIndex="1" />
                <Label fx:id="tags" styleClass="cell_small_label" translateX="10.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              </children>
            </GridPane>
          </children>
        </VBox>
      </children>
    </GridPane>
  </children>
</HBox>
