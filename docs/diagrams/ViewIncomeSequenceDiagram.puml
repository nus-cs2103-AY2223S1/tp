@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "v:PinCommand" as PinCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "personToPin:Person" as Person MODEL_COLOR
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(pin)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(viewIncome)
activate AddressBookParser

create PinCommand
AddressBookParser -> PinCommand
activate PinCommand

PinCommand --> AddressBookParser
deactivate PinCommand

AddressBookParser --> LogicManager : p
deactivate AddressBookParser

LogicManager -> PinCommand : execute()
activate PinCommand

PinCommand -> Model : getFilteredPersonList()
activate Model

Model --> PinCommand
deactivate Model

PinCommand -> Person:setPin(!personToPin.getPin())
activate Person
Person --> PinCommand
deactivate Person

PinCommand --> LogicManager : result
deactivate PinCommand
PinCommand -[hidden]-> LogicManager : result


[<--LogicManager
deactivate LogicManager
@enduml
