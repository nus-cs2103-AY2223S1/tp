@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title After command "delete -s i/A0123456A"

package States <<rectangle>> {
    class State1 as "__ab0:FypManager__"
    class State2 as "__ab1:FypManager__"
    class State3 as "__ab2:FypManager__"
}

State1 -[hidden]right-> State2
State2 -[hidden]right-> State3

hide State3

class Pointer as "Current State" #FFFFFF

Pointer -up-> State2
@end
