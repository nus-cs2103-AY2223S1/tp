@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":HealthContactParser" as HealthContactParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedHealthContact" as VersionedHealthContact MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> HealthContactParser : parseCommand(undo)
activate HealthContactParser

create UndoCommand
HealthContactParser -> UndoCommand
activate UndoCommand

UndoCommand --> HealthContactParser
deactivate UndoCommand

HealthContactParser --> LogicManager : u
deactivate HealthContactParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoHealthContact()
activate Model

Model -> VersionedHealthContact : undo()
activate VersionedHealthContact

VersionedHealthContact -> VersionedHealthContact :resetData(ReadOnlyHealthContact)
VersionedHealthContact --> Model :
deactivate VersionedHealthContact

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
