{
  "MD004": { // Unordered list style
    "style": "asterisk"
  },
  "MD009": { // Trailing spaces
    "br_spaces": 0
  },
  "MD013": false, // Line length
  "MD029": { // Ordered list item prefix
    "style": "one"
  },
  "MD033": { // Inline HTML
    "allowed_elements": ["sub", "sup", "br", "kbd"]
  },
